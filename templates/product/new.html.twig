{% extends 'layout.html.twig' %}
{% block content %}
<h1>Register of a New Product</h1>


{{ form_start(form) }}

<div class="form-control col-md-4">

                {{ form_row(form.title) }}
    </div>
    <div class="form-control col-md-4">
                {{ form_row(form.content) }}
    </div>

                {{ form_row(form.created_at) }}

    <div class="form-control col-md-4">
                {{ form_row(form.updated_at) }}
    </div>
    <div class="form-control col-md-4">
        {{ form_row(form.is_enabled) }}
    </div>
    <div class="form-control col-md-4">
        {{ form_row(form.nb_like) }}
    </div>
    <div class="form-control col-md-4">
        <button type = "submit" class="btn btn-info">Validate</button>
    </div>

{{ form_end(form) }}

{% endblock %}





